    <query_expression>
        <![CDATA[SELECT ALL
   t0.P1_proposal_id AS P1_proposal_id, 
   t1.R1_name AS R1_name, 
   t1.R1_proposal_ref AS R1_proposal_ref, 
   t2.P2_proposal_id AS P2_proposal_id, 
   t3.R2_name AS R2_name, 
   t3.R2_proposal_ref AS R2_proposal_ref
FROM
         (
            SELECT ALL
               P1.proposal_id AS P1_proposal_id
            FROM
               public.proposals AS P1
         ) AS t0
         RIGHT OUTER JOIN 
         (
            SELECT ALL
               R1.name AS R1_name, 
               R1.proposal_ref AS R1_proposal_ref
            FROM
               public.reviewers AS R1
         ) AS t1
         ON       (t0.P1_proposal_id = t1.R1_proposal_ref)

      FULL OUTER JOIN 
         (
            SELECT ALL
               P2.proposal_id AS P2_proposal_id
            FROM
               public.proposals AS P2
         ) AS t2
         LEFT OUTER JOIN 
         (
            SELECT ALL
               R2.name AS R2_name, 
               R2.proposal_ref AS R2_proposal_ref
            FROM
               public.reviewers AS R2
         ) AS t3
         ON       (t2.P2_proposal_id = t3.R2_proposal_ref)

      ON    (t0.P1_proposal_id = t3.R2_proposal_ref)]]>
    </query_expression>
