<?xml version="1.0" encoding="UTF-8"?>
<test_case>
	<query_expression>
        <![CDATA[
        	CAST((
	        	SELECT 	*
	        	FROM	
	        		(
        				SELECT 	TUPLE(p.*) as x,
	        					'other attr' as other
	        			FROM	src_1.proposals as p
        			) as p
        		) as TUPLE)
        ]]>
	</query_expression>
	<data_source name="src_1" storage_system="INMEMORY" data_model="RELATIONAL" />
	<data_object name="proposals" execution_data_source="src_1" cardinality_estimate="LARGE">
		<schema_tree>
			<root type="collection">
				<element type="tuple">
					<proposal_id type="integer" />
					<title type="integer" />
				</element>
				<constraints>
					<local-key collection="/">
						<attribute path="tuple/proposal_id" />
					</local-key>
				</constraints>
			</root>
		</schema_tree>
		<data_tree>
			<root>
				<element>
					<proposal_id>1</proposal_id>
					<title>7</title>
				</element>
			</root>
		</data_tree>
	</data_object>
	<data_object name="output" execution_data_source="output">
		<schema_tree>
			<root type="tuple">
				<x type="tuple">
					<proposal_id type="integer" />
					<title type="integer" />
				</x>
				<other type="string"/>
			</root>
		</schema_tree>
		<data_tree>
			<root>
				<x>
					<title>7</title>
					<proposal_id>1</proposal_id>
				</x>
				<other>other attr</other>
			</root>
		</data_tree>
	</data_object>
</test_case>