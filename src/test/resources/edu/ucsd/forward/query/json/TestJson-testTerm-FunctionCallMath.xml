<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <query_expression>
        <![CDATA[
            SELECT mem.json_obj.n1 + 2 AS v1, mem.json_obj.n2 + 4 AS v2,mem.json_obj.n1 + mem.json_obj.n2 AS v3 
        ]]>
    </query_expression>
    <data_object name="json_obj" execution_data_source="mem">
        <schema_tree>
            <root type="json">
            </root>
        </schema_tree>
        <data_tree>
            <root>
                {
                n1: 1,
                n2: 0.2
                }
            </root>
        </data_tree>
    </data_object>
    <data_object name="output" execution_data_source="output">
        <schema_tree>
            <root type="collection">
                <element type="tuple">
                    <v1 type="integer" />
                    <v2 type="float" />
                    <v3 type="json" />
                </element>
            </root>
        </schema_tree>
        <data_tree>
            <root>
                <element>
                    <v1>3</v1>
                    <v2>4.2</v2>
                    <v3>1.2</v3>
                </element>
            </root>
        </data_tree>
    </data_object>
</test_case>