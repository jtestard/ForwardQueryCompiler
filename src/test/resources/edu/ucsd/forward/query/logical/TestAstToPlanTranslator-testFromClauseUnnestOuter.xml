            <root type="collection">
                <element type="tuple">
                    <proposal_id type="integer" />
                    <reviews type="collection">
                        <element type="tuple">
                            <grade type="integer" />
                            <comment type="string" />
                        </element>
                    </reviews>
                </element>
                <constraints>
                    <local-key>
                        <attribute path="tuple/proposal_id" />
                    </local-key>
                </constraints>
            </root>
        </schema_tree>
