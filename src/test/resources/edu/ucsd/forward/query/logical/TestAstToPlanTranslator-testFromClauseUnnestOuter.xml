                                <arguments>
                                    <Variable data_source="src1" mode="ABSOLUTE" schema_object="proposals" />
                                </arguments>
                                <Ground cardinality_estimate="UNKNOWN">
                                    <arguments />
                                </Ground>
                            </Scan>
                        </Scan>
