<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <query_expression>
        <![CDATA[
            SELECT A.proposal_id AS pid, B.grade AS nested_grade, comment AS nested_comment
            FROM OUTER FLATTEN(src1.proposals AS A, A.reviews AS B);
        ]]>
    </query_expression>
    <data_object name="proposals" execution_data_source="src1" cardinality_estimate="LARGE" data_model="RELATIONAL"
