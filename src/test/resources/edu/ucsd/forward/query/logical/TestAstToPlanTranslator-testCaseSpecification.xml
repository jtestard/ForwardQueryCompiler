        <![CDATA[
            SELECT  CASE
                        WHEN R.proposal > 1 THEN R.comment
                        WHEN R.proposal < 0 THEN
                            CASE
                                WHEN R.proposal < -10 THEN R.comment
                                ELSE 'nested case'
                            END
                        ELSE 'outer case'
