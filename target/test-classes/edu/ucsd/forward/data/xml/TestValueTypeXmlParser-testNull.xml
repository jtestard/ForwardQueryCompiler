<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <schema_tree>
        <root type="tuple">
            <applications type="collection">
                <element type="tuple">
                    <date type="string"/>
                    <time type="string"/>
                    <apply type="tuple">
                        <name type="string"/>
                        <applying_for type="switch">
                            <phd type="tuple">
                                <specialization type="string"/>
                                <advisor_choice type="string"/>
                            </phd>
                            <masters type="tuple">
                                <area_of_interest type="string"/>
                            </masters>
                        </applying_for>
                    </apply>
                </element>
                <constraints>
                    <local-key>
                        <attribute path="tuple/date"/>
                    </local-key>
                </constraints>
            </applications>
        </root>
        <constraints/>
    </schema_tree>
    <data_tree>
        <root>
            <applications>
                <element>
                    <date>2009-12-25</date>
                    <time>9:00pm</time>
                    <apply null="true"/>
                </element>
                <element>
                    <date>2009-12-26</date>
                    <time>9:00pm</time>
                    <apply>
                        <name>john</name>
                        <applying_for null="true"/>
                    </apply>
                </element>
            </applications>
        </root>
    </data_tree>
</test_case>