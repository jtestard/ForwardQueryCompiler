<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <query_expression>
        <![CDATA[
            SELECT DISTINCT R.reviewer + 1 AS rev, R.reviewer + 2 AS new_rev, AVG(grade) AS grade
            FROM src1.reviews AS R
            GROUP BY R.reviewer
            ORDER BY rev DESC NULLS FIRST, new_rev, grade
            OFFSET 1 ROWS
            FETCH FIRST 6 ROWS ONLY;
        ]]>
    </query_expression>
