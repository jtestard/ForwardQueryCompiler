                            </Item>
                            <Item nulls="LAST" spec="ASC">
                                <FunctionCall name="+">
                                    <Variable name="__v0" />
                                    <Constant>
                                        <integer>2</integer>
                                    </Constant>
                                </FunctionCall>
                            </Item>
                            <Item nulls="LAST" spec="ASC">
                                <Variable name="avg_call__v2" />
                            </Item>
                        </arguments>
                        <GroupBy cardinality_estimate="UNKNOWN">
                            <arguments>
                                <GroupByItem attr_name="__v0">
                                    <Variable name="__v0" />
                                </GroupByItem>
                                <Aggregate alias="avg_call__v2">
                                    <FunctionCall name="AVG" set_quantifier="ALL">
