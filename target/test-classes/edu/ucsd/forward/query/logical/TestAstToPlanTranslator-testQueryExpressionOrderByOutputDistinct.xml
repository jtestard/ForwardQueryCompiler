                    <grade type="integer" />
                    <reviewer type="integer" />
                </element>
                <constraints>
                    <local-key>
                        <attribute path="tuple/proposal" />
                        <attribute path="tuple/reviewer" />
                    </local-key>
                </constraints>
            </root>
        </schema_tree>
        <data_tree>
            <root>
                <element>
                    <proposal>1</proposal>
                    <comment>Cool proposal!</comment>
                    <grade>11</grade>
                    <reviewer>1</reviewer>
                </element>
                <element>
                    <proposal>2</proposal>
                    <comment>Not so cool...</comment>
                    <grade>1</grade>
                    <reviewer>1</reviewer>
                </element>
                <element>
                    <proposal>2</proposal>
                    <comment>Not bad</comment>
                    <grade>2</grade>
                    <reviewer>2</reviewer>
                </element>
            </root>
        </data_tree>
    </data_object>
