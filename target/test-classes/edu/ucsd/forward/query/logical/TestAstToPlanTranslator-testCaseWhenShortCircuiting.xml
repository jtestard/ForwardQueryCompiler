<test_case>
    <query_expression>
        <![CDATA[SELECT CASE 
                WHEN CAST ((select x > 1) as boolean)
                    THEN (CAST((select 1 / x) as float))
                WHEN CAST ((select x <> 0) as boolean)
                    THEN (CAST((select 1 / x) as float))
                ELSE 
                    CAST((select x) as float)
                END
            FROM 
              {{ { x: 2 }, { x: 1 }, { x: 0 } }} AS p
            WHERE
              CASE 
                WHEN CAST ((select x > 1) as boolean)
                    THEN CAST((select 1 / x = 1 / x) as boolean)
                WHEN CAST ((select x <> 0) as boolean)
                    THEN CAST((select 1 / x = 1 / x) as boolean)
                ELSE 
                    CAST((select x = x) as boolean)
                END
        ]]>
    </query_expression>
