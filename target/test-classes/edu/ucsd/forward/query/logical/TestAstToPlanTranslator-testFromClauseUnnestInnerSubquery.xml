<test_case>
    <query_expression>
        <![CDATA[
            SELECT A.pid AS pid, B.grade AS nested_grade, comment AS nested_comment
            FROM FLATTEN(
              (
                SELECT pid, NEST(grade, comment) AS reviews 
                FROM src1.proposals 
                GROUP BY pid
              ) AS A, 
              A.reviews AS B
            );
        ]]>
    </query_expression>
