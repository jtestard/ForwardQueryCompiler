                    <pid type="integer" />
                    <grade type="integer" />
                    <comment type="string" />
                </element>
            </root>
        </schema_tree>
        <data_tree>
            <root>
                <element>
                    <pid>1</pid>
                    <grade>1</grade>
                    <comment>Poor</comment>
                </element>
                <element>
                    <pid>1</pid>
                    <grade>3</grade>
                    <comment>Good</comment>
