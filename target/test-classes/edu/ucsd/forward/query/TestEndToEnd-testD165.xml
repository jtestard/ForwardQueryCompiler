<?xml version="1.0" encoding="UTF-8"?>
<test_case>
	<query_expression>
        <![CDATA[
            SELECT proposals.pid AS pid, proposals.title AS title
            FROM
            (
                SELECT pid, title
                FROM src_1.proposals AS proposals
            ) AS p
            JOIN
            src_1.proposals AS proposals ON (p.pid = proposals.pid)
        ]]>
	</query_expression>
	<data_source name="src_1" storage_system="INMEMORY" data_model="RELATIONAL" />
	<data_object name="proposals" execution_data_source="src_1" cardinality_estimate="LARGE">
		<schema_tree>
			<root type="collection">
				<element type="tuple">
					<pid type="integer" />
					<title type="integer" />
				</element>
				<constraints>
					<local-key collection="/">
						<attribute path="tuple/pid" />
					</local-key>
				</constraints>
			</root>
		</schema_tree>
		<data_tree>
			<root>
				<element>
					<pid>1</pid>
					<title>7</title>
				</element>
			</root>
		</data_tree>
	</data_object>
	<data_object name="output" execution_data_source="output">
		<schema_tree>
			<root type="collection">
				<element type="tuple">
					<pid type="integer" />
					<title type="integer" />
				</element>
			</root>
		</schema_tree>
		<data_tree>
			<root>
				<element>
					<title>7</title>
					<pid>1</pid>
				</element>
			</root>
		</data_tree>
	</data_object>
</test_case>