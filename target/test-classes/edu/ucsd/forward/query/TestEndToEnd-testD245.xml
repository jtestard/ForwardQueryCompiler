<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <query_expression>
        <![CDATA[
        select  p2.id as id,
                exists(
                    select  *
                    from    p2.results.reports
                ) as has_reports
        from    (
                    select  results, id
                    from    d245_db.page
                ) as p2
        ]]>
    </query_expression>
    <data_source name="d245_db" storage_system="INMEMORY" data_model="RELATIONAL" />
    <data_object name="page" execution_data_source="d245_db" cardinality_estimate="LARGE">
        <schema_tree>
            <root type="collection">
                <element type="tuple">
                    <id type="integer"/>
                    <results type="tuple">
                        <reports type="collection">
                            <element type="tuple">
                                <report_id type="integer" />
                                <articles type="collection">
                                    <element type="tuple">
                                        <article_id type="integer" />
                                    </element>
                                </articles>
                            </element>
                        </reports>
                    </results>
                </element>
            </root>
        </schema_tree>
        <data_tree>
            <root>
                <element>
                    <id>1</id>
                    <results>
                        <reports>
                            <element>
                                <report_id>1</report_id>
                                <articles>
                                    <element>
                                        <article_id>1</article_id>
                                    </element>
                                </articles>
                            </element>
                        </reports>
                    </results>
                </element>
            </root>
        </data_tree>
    </data_object>
    <data_object name="output" execution_data_source="output">
        <schema_tree>
            <root type="collection">
                <element type="tuple">
                    <id type="integer"/>
                    <has_reports type="boolean"/>
                </element>
            </root>
        </schema_tree>
        <data_tree>
            <root>
                <element>
                    <id>1</id>
                    <has_reports>true</has_reports>
                </element>
            </root>
        </data_tree>
    </data_object>
</test_case>