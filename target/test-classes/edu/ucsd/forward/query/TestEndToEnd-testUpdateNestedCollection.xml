<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <query_expression>
        <![CDATA[
ACCESS actual.proposals AS P
WHERE P.proposal > 2
UPDATE P.reviews AS R
SET R.reviewer = 'new name'
        ]]>
    </query_expression>



    <data_object name="proposals" execution_data_source="actual" cardinality_estimate="SMALL">
        <schema_tree>
            <root type="collection">
                <element type="tuple">
                    <proposal type="integer" />
                    <author type="string" />
                    <reviews type="collection">
                        <element type="tuple">
                            <grade type="integer" />
                            <reviewer type="string" />
                        </element>
                        <constraints>
                            <local-key>
                                <attribute path="tuple/reviewer" />
                            </local-key>
                        </constraints>
                    </reviews>
                </element>
                <constraints>
                    <local-key collection="/">
                        <attribute path="tuple/proposal" />
                    </local-key>
                </constraints>
            </root>
        </schema_tree>
        <data_tree>
