<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <query_expression>
        <![CDATA[
            SELECT J.value AS value, CAST(J.color AS string) AS color
            FROM mem.json_obj AS J  
        ]]>
    </query_expression>
    <data_object name="json_obj" execution_data_source="mem">
        <schema_tree>
            <root type="json">
            </root>
        </schema_tree>
        <data_tree>
            <root>
                [
                {
                color: "red",
                value: "#f00"
                },
                {
                color: "green",
                value: "#0f0"
                }
                ]
            </root>
        </data_tree>
    </data_object>
    <data_object name="output" execution_data_source="output">
        <schema_tree>
            <root type="collection">
                <element type="tuple">
                    <value type="json" />
                    <color type="string" />
                </element>
            </root>
        </schema_tree>
        <data_tree>
            <root>
                <element>
                    <value>"#f00"</value>
                    <color>red</color>
                </element>
                <element>
                    <value>"#0f0"</value>
                    <color>green</color>
                </element>
            </root>
        </data_tree>
    </data_object>
</test_case>