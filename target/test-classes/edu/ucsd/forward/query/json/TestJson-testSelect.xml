<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <query_expression>
        <![CDATA[
            SELECT CAST(J.n1 AS integer) AS int
            FROM mem.json_obj AS J 
            WHERE J.n1=1  
        ]]>
    </query_expression>
    <data_object name="json_obj" execution_data_source="mem">
        <schema_tree>
            <root type="json">
            </root>
        </schema_tree>
        <data_tree>
            <root>
                [
                {
                n1: 1,
                n2: "1"
                },
                {
                n1: 0,
                n2: "2"
                },
                {
                n1: 2,
                n2: "3"
                }                                
                ]
            </root>
        </data_tree>
    </data_object>    
    <data_object name="output" execution_data_source="output">
        <schema_tree>
            <root type="collection">
                <element type="tuple">
                    <int type="integer" />
                </element>
            </root>
        </schema_tree>
        <data_tree>
            <root>
                <element>
                    <int>1</int>
                </element>                            
            </root>
        </data_tree>
    </data_object>
</test_case>