<?xml version="1.0" encoding="UTF-8"?>
<test_case>
	<query_expression>
        <![CDATA[
        	SELECT 	CASE WHEN FALSE THEN TUPLE( 1 AS id, 'a' AS name)
        			ELSE TUPLE( 2 AS id, 'b' AS name) end as t  
        ]]>
	</query_expression>
	<data_object name="output" execution_data_source="output">
		<schema_tree>
			<root type="collection">
				<element type="tuple">
					<t type="tuple">
						<id type="integer" />
						<name type="string" />
					</t>
				</element>
			</root>
		</schema_tree>
		<data_tree>
			<root>
				<element>
					<t>
						<id>2</id>
						<name>b</name>
					</t>
				</element>
			</root>
		</data_tree>
	</data_object>
</test_case>