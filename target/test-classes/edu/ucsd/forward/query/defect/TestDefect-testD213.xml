<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <query_expression>
        <![CDATA[
             select resources.id as id, resources.name as name
             from
                (
                    select id, name, resource_type from d213_ide.actions union all
                    select id, name, resource_type from d213_ide.pages
                ) as resources
                -- d213_ide.pages as resources
             where
                d213_temp.my_context.resource_type_id = resources.resource_type and
                d213_temp.my_context.path = resources.name
        ]]>
    </query_expression>
    <data_source name="d213_ide" storage_system="JDBC" data_model="RELATIONAL">
        <properties environment="testing" overwrite="true" user="postgres" password="postgres" maxPoolSize="20"
            unreturnedConnectionTimeout="0" debugUnreturnedConnectionStackTraces="true" driver="postgresql" host="localhost" port="5432"
            database="forward" schema="public" />
        <properties environment="jenkins" overwrite="true" user="postgres" password="postgres" maxPoolSize="20"
            unreturnedConnectionTimeout="0" debugUnreturnedConnectionStackTraces="true" driver="postgresql" host="localhost" port="5432"
            database="forward" schema="public" />
    </data_source>
