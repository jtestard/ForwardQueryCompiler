<?xml version="1.0" encoding="UTF-8"?>
<test_case>
    <query_expression>
        <![CDATA[
            SELECT p.title AS title, INLINE SWITCH WHEN 1=0 THEN INLINE tuple(proposal_id AS proposal_id, '1' AS holder1, tuple('2' AS iholder1) AS tt) AS case1
                                                   END ss
            FROM src_1.proposals AS p
        ]]>
    </query_expression>
    <data_source name="src_1" storage_system="INMEMORY" data_model="RELATIONAL" />
    <data_object name="proposals" execution_data_source="src_1" cardinality_estimate="LARGE">
        <schema_tree>
            <root type="collection">
                <element type="tuple">
                    <proposal_id type="integer" />
                    <title type="integer" />
                </element>
                <constraints>
                    <local-key collection="/">
                        <attribute path="tuple/proposal_id" />
                    </local-key>
                </constraints>
            </root>
        </schema_tree>
        <data_tree>
            <root>
                <element>
                    <proposal_id>1</proposal_id>
                    <title>7</title>
                </element>
            </root>
        </data_tree>
    </data_object>
    <data_object name="output" execution_data_source="output">
        <schema_tree>
            <root type="collection">
                <element type="tuple">
                    <title type="integer" />
                    <proposal_id type="integer" />
                    <holder1 type="string" />
                    <tt type="tuple">
                        <iholder1 type="string" />
                    </tt>
                    <__selected_ss type="string" />
                </element>
            </root>
        </schema_tree>
        <data_tree>
            <root>
                <element>
                    <title>7</title>
                    <proposal_id null="true" />
                    <holder1 null="true" />
                    <tt null="true" />
                    <__selected_ss null="true" />
                </element>
            </root>
        </data_tree>
    </data_object>
</test_case>